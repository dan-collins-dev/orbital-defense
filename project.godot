; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="orbital-defense"
run/main_scene="res://scenes/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

ReferenceManager="*res://utils/ReferenceManager.tscn"
RequestManager="*res://utils/RequestManager.tscn"

[display]

window/size/viewport_width=200
window/size/viewport_height=200
window/size/window_width_override=800
window/size/window_height_override=800
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor]

naming/scene_name_casing=1

[layer_names]

2d_physics/layer_1="SatelliteHurtbox"
2d_physics/layer_2="AsteroidHitbox"
2d_physics/layer_3="PlanetHurtbox"
2d_physics/layer_4="AsteroidHurtbox"
2d_physics/layer_5="ProjectileHitbox"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
